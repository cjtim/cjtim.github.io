---
import ExpCard from "./ExpCard.astro"
import Agoda from "./contents/Agoda.astro"
---

<div
  id="exp"
  class="container mx-auto flex flex-col gap-8 rounded-3xl bg-white p-8"
>
  <p class="text-2xl">
    Professional Experiences
  </p>

  <div class="flex flex-col gap-4 px-8">
    <ExpCard
      heading="DevOps Engineer"
      company="Agoda"
      timeline="April 2022 - July 2023"
      Description={Agoda}
      line
    />
  </div>
</div>
